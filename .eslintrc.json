{
    "extends": [
        "react-app",
        "prettier",
        "prettier/react",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "error",
        "react/prefer-stateless-function": "error"
    }
}